    "@nextui-org/react": "^2.2.9",
    "@prisma/client": "5.7.0",
    "class-validator": "^0.14.0",
    "framer-motion": "^10.16.14",
    "graphql": "^16.8.1",
    "graphql-scalars": "^1.22.4",
    "next": "14.0.3",
    "prisma": "5.7.0",
    "react": "^18",
    "react-dom": "^18",
    "reflect-metadata": "^0.1.13",
    "swiper": "^11.0.5",
    "type-graphql": "2.0.0-beta.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/client-preset": "4.1.0",
