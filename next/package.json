    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "**/*.(ts|tsx|js|jsx)": "bash -c tsc --noEmit",
    "**/*.{js|ts|tsx}": [
      "yarn lint --fix",
      "yarn format"
    ],
    "**/*.(md|json)": "yarn format"
